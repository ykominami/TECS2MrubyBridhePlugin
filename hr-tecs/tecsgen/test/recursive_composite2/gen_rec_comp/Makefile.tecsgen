TECS_IMPORT_CDLS = recursive_composite.cdl gen_rec_comp/tmp_plugin_post_code.cdl
TECS_IMPORT_HEADERS = ../RUNTIME/runtime/include/tecs.h
TECS_IMPORTS = $(TECS_IMPORT_CDLS) $(TECS_IMPORT_HEADERS)

SIGNATURE_HEADERS = \
	$(GEN_DIR)/sSig1_tecsgen.h \
	$(GEN_DIR)/sSig2_tecsgen.h \
	$(GEN_DIR)/sSig3_tecsgen.h \
	$(GEN_DIR)/sSig4_tecsgen.h \
	$(GEN_DIR)/sSig5_tecsgen.h \
	$(GEN_DIR)/sSigB_tecsgen.h \
# SIGNATURE_HEADERS terminator

CELLTYPE_TECSGEN_HEADERS = \
 	$(GEN_DIR)/tCell1_tecsgen.h \
 	$(GEN_DIR)/tCell2_tecsgen.h \
 	$(GEN_DIR)/tCell3_tecsgen.h \
 	$(GEN_DIR)/tCell4_tecsgen.h \
 	$(GEN_DIR)/tCell_serv_tecsgen.h \
 	$(GEN_DIR)/tCell_clnt_tecsgen.h \
# CELLTYPE_TECSGEN_HEADERS terminator

CELLTYPE_FACTORY_HEADERS = \
 	$(GEN_DIR)/tCell1_factory.h \
 	$(GEN_DIR)/tCell2_factory.h \
 	$(GEN_DIR)/tCell3_factory.h \
 	$(GEN_DIR)/tCell4_factory.h \
 	$(GEN_DIR)/tCell_serv_factory.h \
 	$(GEN_DIR)/tCell_clnt_factory.h \
# CELLTYPE_FACTORY_HEADERS terminator

# TECS_HEADERS:  headers generated by tecsgen
TECS_HEADERS = $(SIGNATURE_HEADERS) $(CELLTYPE_TECSGEN_HEADERS) $(CELLTYPE_FACTORY_HEADERS)

TECS_INLINE_HEADERS = \
 	tCell1_tecsgen.h \
 	tCell2_tecsgen.h \
 	tCell3_tecsgen.h \
 	tCell4_tecsgen.h \
 	tCell_serv_tecsgen.h \
 	tCell_clnt_tecsgen.h \
# TECS_INLINE_HEADERS terminator

PLUGIN_INLINE_HEADERS = \
# PLUGIN_INLINE_HEADERS terminator

# TECS_COBJS: all objects of TECS, include both user written code and tecsgen automatically generated code
TECS_COBJS = $(TECSGEN_COBJS) $(PLUGIN_COBJS) $(CELLTYPE_COBJS)

# TECS_COBJS: objects from sources which are automatically generated by 
      
TECSGEN_COBJS = \
 	$(_TECS_OBJ_DIR)tCell1_tecsgen.o \
 	$(_TECS_OBJ_DIR)tCell2_tecsgen.o \
 	$(_TECS_OBJ_DIR)tCell3_tecsgen.o \
 	$(_TECS_OBJ_DIR)tCell4_tecsgen.o \
 	$(_TECS_OBJ_DIR)tCell_serv_tecsgen.o \
 	$(_TECS_OBJ_DIR)tCell_clnt_tecsgen.o \
# TECSGEN_COBJS terminator

# PLUGIN_COBJS: objects from sources which are automatically generated by plugin(s)
PLUGIN_COBJS = \
# PLUGIN_COBJS terminator

# CELLTYPE_COBJS: objects of celltype code written by user
CELLTYPE_COBJS = \
 	$(_TECS_OBJ_DIR)tCell1.o \
 	$(_TECS_OBJ_DIR)tCell2.o \
 	$(_TECS_OBJ_DIR)tCell3.o \
 	$(_TECS_OBJ_DIR)tCell4.o \
 	$(_TECS_OBJ_DIR)tCell_serv.o \
 	$(_TECS_OBJ_DIR)tCell_clnt.o \
# CELLTYPE_COBJS terminator

# TECSGEN_SRCS: sources automatically generated by tecsgen
TECSGEN_SRCS = \
 	$(GEN_DIR)/tCell1_tecsgen.c \
 	$(GEN_DIR)/tCell2_tecsgen.c \
 	$(GEN_DIR)/tCell3_tecsgen.c \
 	$(GEN_DIR)/tCell4_tecsgen.c \
 	$(GEN_DIR)/tCell_serv_tecsgen.c \
 	$(GEN_DIR)/tCell_clnt_tecsgen.c \
